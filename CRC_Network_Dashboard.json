      "title": "CRC average uptime [%]",
      "transformations": [
        {
          "id": "convertFieldType",
          "options": {
            "conversions": [
              {
                "destinationType": "number",
                "targetField": "Time"
              }
            ],
            "fields": {}
          }
        },
        {
          "id": "calculateField",
          "options": {
            "alias": "StartTime",
            "binary": {
              "left": "0",
              "right": "$__from"
            },
            "mode": "binary",
            "reduce": {
              "reducer": "sum"
            }
          }
        },
        {
          "id": "calculateField",
          "options": {
            "alias": "EndTime",
            "binary": {
              "left": "0",
              "right": "$__to"
            },
            "mode": "binary",
            "reduce": {
              "reducer": "sum"
            }
          }
        },
        {
          "id": "calculateField",
          "options": {
            "alias": "Time-StartTime",
            "binary": {
              "left": "Time",
              "operator": "-",
              "right": "StartTime"
            },
            "mode": "binary",
            "reduce": {
              "include": [
                "Time"
              ],
              "reducer": "diff"
            }
          }
        },
        {
          "id": "calculateField",
          "options": {
            "alias": "Time-EndTime",
            "binary": {
              "left": "Time",
              "operator": "-",
              "right": "EndTime"
            },
            "mode": "binary",
            "reduce": {
              "reducer": "sum"
            }
          }
        },
        {
          "id": "filterByValue",
          "options": {
            "filters": [
              {
                "config": {
                  "id": "greater",
                  "options": {
                    "value": 0
                  }
                },
                "fieldName": "Time-StartTime"
              },
              {
                "config": {
                  "id": "lower",
                  "options": {
                    "value": 0
                  }
                },
                "fieldName": "Time-EndTime"
              }
            ],
            "match": "all",
            "type": "include"
          }
        },
        {
          "id": "reduce",
          "options": {
            "reducers": [
              "sum",
              "count"
            ]
          }
        },
        {
          "id": "calculateField",
          "options": {
            "alias": "Uptime Percentage",
            "binary": {
              "left": "Total",
              "operator": "/",
              "right": "Count"
            },
            "mode": "binary",
            "reduce": {
              "reducer": "sum"
            }
          }
        },
        {
          "id": "calculateField",
          "options": {
            "alias": "Uptime Percentage %",
            "binary": {
              "left": "Uptime Percentage",
              "operator": "*",
              "right": "100"
            },
            "mode": "binary",
            "reduce": {
              "reducer": "sum"
            }
          }
        },
        {
          "id": "filterByValue",
          "options": {
            "filters": [
              {
                "config": {
                  "id": "greater",
                  "options": {
                    "value": "0"
                  }
                },
                "fieldName": "Uptime Percentage %"
              }
            ],
            "match": "any",
            "type": "include"
          }
        },
        {
          "id": "filterByValue",
          "options": {
            "filters": [
              {
                "config": {
                  "id": "greater",
                  "options": {
                    "value": 100
                  }
                },
                "fieldName": "Uptime Percentage %"
              }
            ],
            "match": "any",
            "type": "exclude"
          }
        },
        {
